var style = require('./../../libs/wxs/style.wxs');
var addUnit = require('./../../libs/wxs/add-unit.wxs');

function titleStyle(data) {
  return style([
    {
      'max-width': addUnit(data.titleWidth),
      'min-width': addUnit(data.titleWidth),
    },
    data.titleStyle,
  ]);
}

module.exports = {
  titleStyle: titleStyle,
};
